{
  "env": "",
  "url": "http://localhost:6112",
  "path": "/files",
  "mongo": {
    "dbname": "files",
    "url": "mongodb://localhost:27017/",
    "releaseTimeout": 1000
  },
  "redis": {
    "host": "localhost",
    "port": 6379,
    "db": 0,
    "releaseTimeout": 1000
  },
  "cors": {
    "origin": "*"
  },
  "log": {
    "appenders": {
      "default": {
        "type": "console",
        "filename": "log/all.log"
      }
    },
    "categories": {
      "default": {
        "appenders": [
          "default"
        ],
        "level": "all"
      }
    }
  },
  "encrypt": {
    "pwd": "a!b@=;",
    "mode": [
      "query",
      "body",
      "response"
    ],
    "baseOnRequest": true
  },
  "services": {
    "oauth": "http://localhost:6111"
  },
  "app": {
    "scanTimeout": 1000
  }
}