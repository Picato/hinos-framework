{
  "url": "http://localhost:10009",
  "path": "/coin-api",
  "app": {
    "telegram": {
      "OrderBot": "",
      "AlertBot": "",
      "MenuBot": ""
    },
    "bittrex": {
      "scanTimeout": 5000,
      "scanRemitano": 10000
    }
  },
  "log": {
    "appenders": {
      "default": {
        "type": "console",
        "filename": "log/all.log"
      }
    },
    "categories": {
      "default": {
        "appenders": [
          "default"
        ],
        "level": "all"
      }
    }
  },
  "mongo": {
    "dbname": "coin",
    "url": "mongodb://localhost:27017/",
    "releaseTimeout": 1000
  },
  "redis": {
    "host": "localhost",
    "port": 6379,
    "db": 2,
    "releaseTimeout": 1000
  },
  "encrypt": {
    "pwd": ")^!!!(**",
    "mode": [
      "query",
      "body",
      "response"
    ],
    "baseOnRequest": true
  }
}