{
  "env": "",
  "url": "http://localhost:6111",
  "path": "/oauth",
  "mongo": {
    "dbname": "oauth",
    "url": "mongodb://localhost:27017/",
    "releaseTimeout": 1000
  },
  "redis": {
    "host": "localhost",
    "port": 6379,
    "db": 0,
    "releaseTimeout": 1000
  },
  "log": {
    "appenders": {
      "default": {
        "type": "console",
        "filename": "log/all.log"
      }
    },
    "categories": {
      "default": {
        "appenders": [
          "default"
        ],
        "level": "all"
      }
    }
  },
  "cors": {
    "exposeHeaders": [
      "token",
      "project_id",
      "account_id"
    ],
    "origin": "*"
  },
  "encrypt": {
    "pwd": "",
    "mode": [],
    "baseOnRequest": true
  },
  "services": {
    "oauth": "http://localhost:6111",
    "mail": "http://localhost:6113"
  },
  "app": {
    "suid": "123456789",
    "twoStepVerificationTimeout": 60000
  }
}