{
  "name": "hinos-framework",
  "version": "1.0.0",
  "description": "hinos project which is built for APIs",
  "main": ".build/index.js",
  "scripts": {
    "once": "npm install -g typescript && npm install -g cross-var && npm install -g tslint-config-standard",
    "start": "node .",
    "prebuild": "rimraf .build .spec",
    "build": "tsc",
    "pretest": "rimraf .spec && tsc -p src/test/tsconfig.json",
    "test": "mocha --opts src/test/mocha.opts \".spec/test/spec/*.js\"",
    "dev": "tsc --watch --pretty --sourceMap",
    "pm2": "cross-var \"pm2 start . --env production --name $npm_package_name\"",
    "gencode": "git clone https://github.com/doanthuanthanh88/quickstart.git -b hinos@gencode --single-branch gencode && cd gencode && npm install",
    "gen": "cd gencode && npm start"
  },
  "appconfig": [
    "appconfig.json",
    "global.json?mongo,redis,encrypt,#api"
  ],
  "dependencies": {
    "hinos": "^1.1.6",
    "hinos-bodyparser": "^2.2.3",
    "hinos-common": "^1.0.8",
    "hinos-cors": "^1.0.2",
    "hinos-helmet": "^1.0.0",
    "hinos-mongo": "^1.2.4",
    "hinos-rabbitmq": "^2.0.14",
    "hinos-redis": "^1.1.11",
    "hinos-requesthasher": "^1.1.18",
    "hinos-requestmatcher": "^1.0.7",
    "hinos-route": "^1.1.14",
    "hinos-serve": "^1.1.2",
    "hinos-socketio": "^2.0.11",
    "hinos-validation": "^1.1.2",
    "lodash": "^4.17.4",
    "typescript": "^2.7.1"
  },
  "author": "doanthuanthanh88@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^3.5.2",
    "@types/lodash": "^4.14.99",
    "@types/mocha": "^2.2.48",
    "@types/node": "^7.0.52",
    "axios": "^0.16.2",
    "chai": "^3.5.0",
    "mocha": "^3.5.3",
    "rimraf": "^2.6.2"
  }
}