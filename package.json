{
  "name": "hinos-service",
  "version": "1.0.0",
  "description": "hinos project which is built for APIs",
  "main": ".build/index.js",
  "scripts": {
    "once": "npm install -g typescript && npm install -g cross-var && npm install -g tslint-config-standard",
    "start": "node .",
    "prebuild": "rimraf .build .spec",
    "build": "tsc",
    "pretest": "rimraf .spec && tsc -p src/test/tsconfig.json",
    "test": "mocha --opts src/test/mocha.opts \".spec/test/spec/*.js\"",
    "dev": "tsc --watch --pretty --sourceMap",
    "pm2": "cross-var \"npm run build && pm2 start . --env production --name $npm_package_name\""
  },
  "config": {
    "externalConfig": "./appconfig.cfg#api",
    "routes": {},
    "url": "http://localhost:1007",
    "services": {
      "oauth": "http://localhost:1001"
    },
    "encryptKey": ")^!!!(**",
    "mongo": {
      "url": "mongodb://localhost:27017/test",
      "releaseTimeout": 50000
    },
    "redis": {
      "host": "localhost",
      "port": 6379,
      "releaseTimeout": 50000
    }
  },
  "dependencies": {
    "hinos": "^1.1.0",
    "hinos-bodyparser": "^1.1.1",
    "hinos-common": "^1.0.5",
    "hinos-cors": "^1.0.0",
    "hinos-helmet": "^1.0.0",
    "hinos-mongo": "^1.0.7",
    "hinos-rabbitmq": "^1.0.4",
    "hinos-redis": "^1.0.4",
    "hinos-requestmatcher": "^1.0.5",
    "hinos-route": "^1.1.3",
    "hinos-serve": "^1.1.0",
    "hinos-socketio": "^1.0.3",
    "hinos-validation": "^1.1.1",
    "lodash": "^4.17.4"
  },
  "author": "doanthuanthanh88@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/lodash": "^4.14.71",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.39",
    "chai": "^3.5.0",
    "mocha": "^3.3.0",
    "rimraf": "^2.6.1"
  }
}